{
	"manifest_version": 3,
	"name": "MDBitwarden",
	"description": "Markdown Formatted Secure Notes for Bitwarden",
	"version": "1.2.1",
	"icons": {
		"16": "logo16.png",
		"32": "logo32.png",
		"48": "logo48.png",
		"128": "logo128.png"
	},
	"content_scripts": [
		{
			"matches": ["https://*/*"],
			"css": ["styles.css"],
			"js": ["showdown.js", "content.js"]
		}
	]
}
